{
  "name": "launchts",
  "version": "1.0.1",
  "description": "Fast, interactive CLI to scaffold TypeScript projects",
  "bin": {
    "launchts": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node src/cli.ts",
    "test": "vitest",
    "lint": "eslint .",
    "prepare": "husky install",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "keywords": [
    "typescript",
    "scaffold",
    "cli"
  ],
  "author": "Jonas Szigeti <jonas.szigeti@icloud.com>",
  "license": "MIT",
  "dependencies": {
    "cac": "^6.7.2",
    "enquirer": "^2.3.6",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.0.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  }
}
